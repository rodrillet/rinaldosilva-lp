# Configurações de produção
NODE_ENV=production
PORT=3000
HOSTNAME=0.0.0.0

# Desabilitar telemetria do Next.js
NEXT_TELEMETRY_DISABLED=1

# URLs da aplicação
NEXT_PUBLIC_APP_URL=https://rinaldosilva.com
NEXT_PUBLIC_API_URL=https://rinaldosilva.com/api

# Configurações do Facebook (substitua pelos valores reais)
# NEXT_PUBLIC_FACEBOOK_PIXEL_ID=your_pixel_id
# FACEBOOK_ACCESS_TOKEN=your_access_token
# FACEBOOK_TEST_EVENT_CODE=your_test_event_code

# Google Analytics (se aplicável)
# NEXT_PUBLIC_GA_MEASUREMENT_ID=your_ga_id

# Configurações de cache
NEXT_REVALIDATE_TOKEN=your_revalidate_token 